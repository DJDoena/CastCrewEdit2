<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>net472</TargetFrameworks>
		<UseWPF>true</UseWPF>
		<GenerateResourceUsePreserializedResources>true</GenerateResourceUsePreserializedResources>
		<GenerateAssemblyInfo>false</GenerateAssemblyInfo>
		<Platforms>x86;x64</Platforms>
		<OutputType>WinExe</OutputType>
		<RootNamespace>DoenaSoft.DVDProfiler.FindMergedCastCrew</RootNamespace>
		<AssemblyName>FindMergedCastCrew</AssemblyName>
		<GenerateDocumentationFile>False</GenerateDocumentationFile>
		<ApplicationIcon>djdsoft.ico</ApplicationIcon>
	</PropertyGroup>

	<ItemGroup>
	  <None Remove="djdsoft.ico" />
	</ItemGroup>

	<ItemGroup>
	  <PackageReference Include="DoenaSoft.AbstractionLayer.IO" Version="1.0.2" />
	  <PackageReference Include="DoenaSoft.AbstractionLayer.WPF" Version="1.1.4" />
	  <PackageReference Include="DoenaSoft.WindowsAPICodePack" Version="1.1.2" />
	</ItemGroup>

	<ItemGroup>
	  <ProjectReference Include="..\CastCrewEdit2Library\CastCrewEdit2Library.csproj" />
	</ItemGroup>

	<ItemGroup>
	  <Resource Include="djdsoft.ico" />
	</ItemGroup>

</Project>